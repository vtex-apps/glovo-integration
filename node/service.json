{
  "memory": 1024,
  "ttl": 60,
  "timeout": 5,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "createOrder": {
      "path": "/_v/glovo/orders/create",
      "public": true
    },
    "cancelOrder": {
      "path": "/_v/glovo/orders/delete",
      "public": true
    },
    "getJSON": {
      "path": "/_v/glovo/json/get",
      "public": false
    },
    "saveJSON": {
      "path": "/_v/glovo/json/save",
      "public": false
    },
    "getOrderRecord": {
      "path": "/_v/glovo/order-records/get",
      "public": false
    },
    "updateProduct": {
      "path": "/_v/glovo/products/update",
      "public": true
    },
    "updateMenuPartial": {
      "path": "/_v/glovo/menu/update-partial",
      "public": true
    },
    "updateMenuAll": {
      "path": "/_v/glovo/menu/update-all",
      "public": true
    },
    "createGlovoMenuRecord": {
      "path": "/_v/glovo/menu-record/create",
      "public": true
    }
  },
  "events": {
    "updateOnStartHandling": {
      "sender": "vtex.orders-broadcast",
      "topics": ["handling"]
    },
    "updateOnInvoiced": {
      "sender": "vtex.orders-broadcast",
      "topics": ["invoiced"]
    }
  }
}
